{
    "manifest_version": 3,
    "name": "WhatsBoost",
    "version": "1.0",
    "description": "Redirects to WhatsBoost dashboard",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs"
    ],
    "action": {
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
      "default_title": "Go to WhatsBoost Dashboard"
    }
  }
  